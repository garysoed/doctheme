<div class="class">
  <section class="class-summary">
    {{#ifeq module 'tutorial'}}
      <header class="tutorial">
        <h2>{{moduleName}}</h2>
      </header>
    {{else}}
      <header>
        <code>{{#if access}}<span class="keyword">{{access}}</span> {{/if}}{{#if static}}<span class="keyword">static</span> {{/if}}{{moduleName}}{{#if extends}} <span class="keyword">extends</span> {{#crossLink extends}}{{/crossLink}}{{/if}}</code>
      </header>
    {{/ifeq}}
    {{#if since}}
      <div class="since">
        since {{since}}
      </div>
    {{/if}}

    <div class="content description">
      {{#if deprecated}}
        <footer class="warn">
          DEPRECATED {{#if deprecationMessage}} - use {{#crossLink deprecationMessage}}{{/crossLink}}{{/if}}
        </footer>
      {{/if}}
      {{{classDescription}}}
    </div>

  </section>

  {{#unless access}}
  {{#if is_constructor}}
    <section class="collapsible class-subsection">
      <header class="reverse">
        <h2 class="button primary">Constructor</h2>
      </header>
      <div>
        {{#is_constructor}}
          {{>method}}
        {{/is_constructor}}
      </div>
    </section>
  {{/if}}
  {{/unless}}

  {{#if properties}}
    <section class="collapsible class-subsection">
      <header class="reverse">
        <h2 class="button primary">Properties</h2>
      </header>
      <div>
        {{#properties}}
          {{>props}}
        {{/properties}}
      </div>
    </section>
  {{/if}}

  {{#if methods}}
    <section class="collapsible class-subsection">
      <header class="reverse">
        <h2 class="button primary">Methods</h2>
      </header>
      <div>
        {{#methods}}
          {{>method}}
        {{/methods}}
      </div>
    </section>
  {{/if}}

  {{#if events}}
    <section class="collapsible class-subsection">
      <header class="reverse">
        <h2 class="button primary">Events</h2>
      </header>
      <div>
        {{#events}}
          {{>events}}
        {{/events}}
      </div>
    </section>
  {{/if}}
</div>

{{!-- {{#if uses}}
  Uses:
  {{#each uses}}
      <a href="{{this}}.html">{{this}}</a>
  {{/each}}
  <br>
{{/if}}
{{#if extension_for}}
    Extension For:
    {{#each extension_for}}
        <a href="{{this}}.html">{{this}}</a>
    {{/each}}
    <br>
{{/if}}
{{#if extends}}
    Extends: {{#crossLink extends}}{{/crossLink}}<br>
{{/if}}
<span class="foundat">Class defined in: <a href="../{{foundAt}}">`{{file}}:{{line}}`</a></span>
<div class="intro">{{{classDescription}}}</div> --}}


