<section class="events">
  <header>
    <code class="method-signature">{{name}}</code>
  </header>
  <div class="content">
    <span class="description">{{{eventDescription}}}</span>
    {{#if params}}
      <table>
      {{#params}}
        <tbody>
          <tr>
            <td colspan="2">
              <p>{{#if optional}}`[{{name}}{{#if optdefault}}={{optdefault}}{{/if}}]`{{else}}`{{name}}`{{/if}}</p>
            </td>
            <td class="method-type">
              <p><code>{{#crossLink type}}{{/crossLink}}</code></p>
            </td>
            <td class="description">
              {{{description}}}
            </td>
          </tr>

          {{#if props}}
          {{#props}}
            <tr>
              <td></td>
              <td>
                <p>{{#if optional}}`[{{name}}{{#if optdefault}}={{optdefault}}{{/if}}]`{{else}}`{{name}}`{{/if}}</p>
              </td>
              <td class="method-type">
                <p><code>{{#crossLink type}}{{/crossLink}}</code></p>
              </td>
              <td class="description">
                {{{description}}}
              </td>
            </tr>
          {{/props}}
          {{/if}}
        </tbody>
      {{/params}}
      </table>
    {{/if}}
  </div>
</section>

